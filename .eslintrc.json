{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": "off",
        "import/no-unresolved": "off",
        "no-console": "off"
    },
    "overrides": [{
        "files": ["**/*.ts"],
        "parser": "@typescript-eslint/parser",
        "rules": {
            "no-undef": "off",
            "no-unused-vars": "off",
            "strict": "off",
            "import/prefer-default-export": "off",
            "import/extensions": "off"
        }
    }],
    "env": {
        "browser": true
    }
}
